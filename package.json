{
  "name": "node-live-sampling",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "engines": {
    "npm": "6.4.1",
    "node": "10.7.0"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && yarn install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SecureCap/node-live-sampling.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SecureCap/node-live-sampling/issues"
  },
  "homepage": "https://github.com/SecureCap/node-live-sampling#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^3.0.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "busboy": "^0.2.14",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.2.17",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "react-s3-uploader": "^4.8.0",
    "yarn": "^1.10.1",    
    "if-env": "^1.0.4"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  }
}
